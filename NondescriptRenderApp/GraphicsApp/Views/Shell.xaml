<Window x:Class="GraphicsApp.Views.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphicsApp.Views"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="Shell" Height="639" Width="1000" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <local:GraphicsUI></local:GraphicsUI>
            <local:GraphicsCanvas></local:GraphicsCanvas>
        </StackPanel>
    </Grid>
</Window>
